<?xml version="1.0" encoding="utf-8"?>
<layer-list android:opacity="opaque"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/white"></item> // color de fondo
    <item
        android:top="100dp"
        android:left="20dp"
        android:right="20dp"
        android:bottom="200dp">
        <bitmap android:src="@drawable/logocalcu"
            android:gravity="center">
        </bitmap>
    </item>

    <item android:drawable="@drawable/logoflott"
        android:height="350px"
        android:gravity="bottom"
        android:bottom="100px">

    </item>
</layer-list>

